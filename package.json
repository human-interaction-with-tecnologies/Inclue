{
  "name": "inclue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pa11y-csv-1-WCAG2A": "pa11y --reporter csv --standard WCAG2A http://localhost:3001/ > results-1-WCAG2A.csv",
    "pa11y-csv-1-WCAG2AA": "pa11y --reporter csv --standard WCAG2AA http://localhost:3001/ > results-1-WCAG2AA.csv",
    "pa11y-csv-1-WCAG2AAA": "pa11y --reporter csv --standard WCAG2AAA http://localhost:3001/ > results-1-WCAG2AAA.csv",
    "pa11y-csv-2-WCAG2A": "pa11y --reporter csv --standard WCAG2A http://localhost:3001/inclue/considerations > results-2-WCAG2A.csv",
    "pa11y-csv-2-WCAG2AA": "pa11y --reporter csv --standard WCAG2AA http://localhost:3001/inclue/considerations > results-2-WCAG2AA.csv",
    "pa11y-csv-2-WCAG2AAA": "pa11y --reporter csv --standard WCAG2AAA http://localhost:3001/inclue/considerations > results-2-WCAG2AAA.csv",
    "pa11y-csv-3-WCAG2A": "pa11y --reporter csv --standard WCAG2A http://localhost:3001/inclue/consideration?id=1 > results-3-WCAG2A.csv",
    "pa11y-csv-3-WCAG2AA": "pa11y --reporter csv --standard WCAG2AA http://localhost:3001/inclue/consideration?id=1 > results-3-WCAG2AA.csv",
    "pa11y-csv-3-WCAG2AAA": "pa11y --reporter csv --standard WCAG2AAA http://localhost:3001/inclue/consideration?id=1 > results-3-WCAG2AAA.csv",
    "pa11y-csv-4-WCAG2A": "pa11y --reporter csv --standard WCAG2A http://localhost:3001/inclue/questions > results-4-WCAG2A.csv",
    "pa11y-csv-4-WCAG2AA": "pa11y --reporter csv --standard WCAG2AA http://localhost:3001/inclue/questions > results-4-WCAG2AA.csv",
    "pa11y-csv-4-WCAG2AAA": "pa11y --reporter csv --standard WCAG2AAA http://localhost:3001/inclue/questions > results-4-WCAG2AAA.csv",
    "pa11y-csv-5-WCAG2A": "pa11y --reporter csv --standard WCAG2A http://localhost:3001/inclue/about > results-5-WCAG2A.csv",
    "pa11y-csv-5-WCAG2AA": "pa11y --reporter csv --standard WCAG2AA http://localhost:3001/inclue/about > results-5-WCAG2AA.csv",
    "pa11y-csv-5-WCAG2AAA": "pa11y --reporter csv --standard WCAG2AAA http://localhost:3001/inclue/about > results-5-WCAG2AAA.csv",
    "pa11y-html-1-WCAG2AA": "pa11y --reporter html --standard WCAG2AA http://localhost:3001/ > results-1-WCAG2AA.html",
    "pa11y-html-2-WCAG2AA": "pa11y --reporter html --standard WCAG2AA http://localhost:3001/inclue/considerations > results-2-WCAG2AA.html",
    "pa11y-html-3-WCAG2AA": "pa11y --reporter html --standard WCAG2AA http://localhost:3001/inclue/consideration?id=1 > results-3-WCAG2AA.html",
    "pa11y-html-4-WCAG2AA": "pa11y --reporter html --standard WCAG2AA http://localhost:3001/inclue/questions > results-4-WCAG2AA.html",
    "pa11y-html-5-WCAG2AA": "pa11y --reporter html --standard WCAG2AA http://localhost:3001/inclue/about > results-5-WCAG2AA.html",

    "pa11y-csv-persona-WCAG2AA": "pa11y --reporter csv --standard WCAG2AA http://localhost:3001/inclue/personas > results-persona-WCAG2AA.csv"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "14.0.3",
    "pa11y-reporter-html": "^2.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
