{
  "name": "inclue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pa11y-html": "pa11y --reporter html http://localhost:3001/ > results.html",
    "pa11y-csv-1": "pa11y --reporter csv http://localhost:3001/ > 1-results-index.csv",
    "pa11y-csv-2": "pa11y --reporter csv http://localhost:3001/inclue/considerations > 2-results-considerations.csv",
    "pa11y-csv-3": "pa11y --reporter csv http://localhost:3001/inclue/consideration/1 > 3-results-consideration.csv",
    "pa11y-csv-4": "pa11y --reporter csv http://localhost:3001/inclue/personas > 4-results-personas.csv",
    "pa11y-csv-5": "pa11y --reporter csv http://localhost:3001/inclue/questions > 5-results-questions.csv",
    "pa11y-csv-6": "pa11y --reporter csv http://localhost:3001/inclue/about > 6-results-about.csv",
    "pa11y-ci": "pa11y-ci --standard WCAG2AA http://localhost:3001/",
    "start-detached": "bundle exec jekyll serve --detach",
    "pa11y-ci:sitemap": "pa11y-ci --sitemap http://localhost:4000/sitemap.xml --sitemap-exclude \"/*.pdf\""
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "14.0.3",
    "pa11y-ci-reporter-html": "^6.0.2",
    "pa11y-reporter-html": "^2.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "pa11y-ci": "^3.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
